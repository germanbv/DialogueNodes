; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Character",
"language": "GDScript",
"path": "res://addons/dialogue_nodes/objects/Character.gd"
}, {
"base": "Resource",
"class": "CharacterList",
"language": "GDScript",
"path": "res://addons/dialogue_nodes/objects/CharacterList.gd"
}, {
"base": "PopupDialog",
"class": "DialogueBox",
"language": "GDScript",
"path": "res://addons/dialogue_nodes/objects/dialogueBox.gd"
}, {
"base": "Resource",
"class": "DialogueData",
"language": "GDScript",
"path": "res://addons/dialogue_nodes/objects/DialogueData.gd"
}, {
"base": "RichTextEffect",
"class": "RichTextWait",
"language": "GDScript",
"path": "res://addons/dialogue_nodes/objects/bbcodeWait.gd"
} ]
_global_script_class_icons={
"Character": "",
"CharacterList": "",
"DialogueBox": "",
"DialogueData": "",
"RichTextWait": ""
}

[application]

config/name="DialogueNodes"
run/main_scene="res://examples/Demo.tscn"
config/icon="res://icon.png"

[display]

window/handheld/orientation="sensor"
window/stretch/aspect="keep"

[locale]

translation_remaps={
"res://examples/ExampleBasic.tres": PoolStringArray( "res://examples/ExampleBasic_ja.tres:ja" )
}

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.0705882, 0.305882, 0.537255, 1 )
environment/default_environment="res://default_env.tres"
